generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Task {
  id        String    @id @default(uuid())
  title     String
  completed Boolean   @default(false)
  order     Int
  dueDate   DateTime?
  priority  String    @default("medium")
  createdAt DateTime  @default(now())
}